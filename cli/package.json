{
  "name": "@asterai/cli",
  "description": "CLI for building and deploying asterai plugins",
  "version": "0.2.0",
  "author": "asterai <support@asterai.io>",
  "repository": "asterai-io/asterai-sdk",
  "homepage": "https://github.com/asterai-io/asterai-sdk",
  "keywords": [
    "asterai",
    "cli",
    "oclif"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky cli/.husky",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "format": "prettier --write .",
    "format-staged": "pretty-quick --staged"
  },
  "bin": {
    "asterai": "./bin/run.js"
  },
  "bugs": "https://github.com/asterai-io/asterai-sdk/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/asterai-init-plugin"
  ],
  "oclif": {
    "bin": "asterai",
    "dirname": "asterai",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "assemblyscript": "^0.27.27",
    "axios": "^1.7.2",
    "form-data": "^4.0.0",
    "mustache": "^4.2.0",
    "protobufjs": "^7.4.0",
    "tempy": "^3.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/mustache": "^4.2.5",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "husky": "^9.0.11",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  }
}
